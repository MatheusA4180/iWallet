<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.iwallet" >

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".utils.di.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_custon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_custon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.IWallet" >
        <activity android:name=".features.resume.ProductsActivity" />
        <activity android:name=".features.HomeActivity" />
        <activity
            android:name=".features.intro.IntroActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>